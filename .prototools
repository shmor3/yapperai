node = "latest"
npm = "11.4.1"
pnpm = "latest"
rust = "1.87.0"

[env]

[plugins]
buf = "https://raw.githubusercontent.com/stk0vrfl0w/proto-toml-plugins/main/plugins/buf.toml"

[settings]
auto-clean = true
auto-install = true
builtin-plugins = true
detect-strategy = "prefer-prototools"
pin-latest = "local"
telemetry = false

[settings.offline]
timeout = 500
