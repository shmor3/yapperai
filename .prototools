node = "24.4.0"
npm = "11.2.0"
pnpm = "10.13.1"
rust = "1.86.0"
buf = "1.50.0"

[env]
DEBUG = "*"
RUST_LOG = "trace"

[plugins]
buf = "https://raw.githubusercontent.com/stk0vrfl0w/proto-toml-plugins/main/plugins/buf.toml"

[settings]
auto-clean = true
auto-install = true
builtin-plugins = true
detect-strategy = "prefer-prototools"
pin-latest = "local"
telemetry = false

[settings.offline]
timeout = 500
